package model;

public class Arbol {
	private int ID;
	private String Especie;
	private double Altura;
	private double DAP;
	private double PrecioEspecie;
	private double ValorEconomico;

	public int getID() {
		return ID;
	}

	public void setID(int ID) {
		this.ID = ID;
	}

	public String getEspecie() {
		return Especie;
	}

	public void setEspecie(String Especie) {
		this.Especie = Especie;
	}

	public double getAltura() {
		return Altura;
	}

	public void setAltura(double Altura) {
		this.Altura = Altura;
	}

	public double getDAP() {
		return DAP;
	}

	public void setDAP(double DAP) {
		this.DAP = DAP;
	}

	public double getPrecioEspecie() {
		return PrecioEspecie;
	}

	public void setPrecioEspecie(double PrecioEspecie) {
		this.PrecioEspecie = PrecioEspecie;
	}

	public double getValorEconomico() {
		return ValorEconomico;
	}

	public void setValorEconomico(double ValorEconomico) {
		this.ValorEconomico = ValorEconomico;
	}
}
